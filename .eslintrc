{
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "env": {
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "strict": 0,
    "no-console": 1,
    "no-unused-vars": 0,
    "prettier/prettier": [2, {"singleQuote": true}],
    "no-use-before-define": [2, "nofunc"],
    "typescript/no-unused-vars": 2
  },
  "plugins": ["prettier", "typescript"]
}
