{
  "name": "esnext",
  "description": "Update your project to the latest ECMAScript syntax.",
  "main": "dist/esnext.js",
  "jsnext:main": "dist/esnext.mjs",
  "bin": {
    "esnext": "./bin/index.js"
  },
  "scripts": {
    "lint": "eslint src test/test.js test/support",
    "pretest": "npm run lint",
    "test": "mocha",
    "prebuild": "npm run lint && rm -rf dist",
    "build": "rollup -c",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "es6",
    "es7",
    "ast"
  ],
  "files": [
    "dist",
    "src",
    "bin"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "Brian Donovan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/esnext/esnext.git"
  },
  "bugs": "https://github.com/esnext/esnext/issues",
  "dependencies": {
    "babel-traverse": "^6.21.0",
    "babel-types": "^6.21.0",
    "babylon": "^6.14.1",
    "magic-string": "^0.19.0",
    "mkdirp": "^0.5.1",
    "shebang-regex": "^2.0.0",
    "strip-indent": "^2.0.0"
  },
  "devDependencies": {
    "assert-jsnext": "^1.3.0",
    "babel": "^6.5.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^3.10.0",
    "eslint-plugin-flow-vars": "^0.4.0",
    "mocha": "^2.5.3",
    "mversion": "^1.10.1",
    "rollup": "^0.33.2",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-npm": "^2.0.0",
    "source-map-support": "^0.4.6",
    "semantic-release": "^4.3.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
