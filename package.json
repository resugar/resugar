{
  "name": "esnext",
  "description": "Update your project to the latest ECMAScript syntax.",
  "main": "dist/esnext.js",
  "jsnext:main": "dist/esnext.mjs",
  "types": "index.d.ts",
  "bin": {
    "esnext": "./bin/index.js"
  },
  "scripts": {
    "lint": "eslint rollup.config.js src test/test.js test/support",
    "pretest": "yarn lint",
    "test": "mocha",
    "prebuild": "yarn lint && rm -rf dist",
    "build": "rollup -c",
    "prepublish": "yarn build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "es6",
    "es7",
    "ast"
  ],
  "files": [
    "dist",
    "src",
    "bin",
    "index.d.ts"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "Brian Donovan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/esnext/esnext.git"
  },
  "bugs": "https://github.com/esnext/esnext/issues",
  "dependencies": {
    "@babel/parser": "^7.3.3",
    "@babel/traverse": "^7.2.3",
    "@babel/types": "^7.3.3",
    "@types/babel-traverse": "^6.25.5",
    "magic-string": "^0.25.2",
    "mkdirp": "^0.5.1",
    "shebang-regex": "^2.0.0",
    "strip-indent": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-syntax-flow": "^7.2.0",
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@types/mz": "^0.0.32",
    "@types/node": "^11.9.4",
    "@types/ora": "^1.3.2",
    "babel-eslint": "^10.0.1",
    "browserify": "^16.1.0",
    "eslint": "^4.18.1",
    "eslint-plugin-flow-vars": "^0.5.0",
    "mocha": "^4.0.1",
    "mz": "^2.7.0",
    "ora": "^2.0.0",
    "rollup": "^0.56.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-npm": "^2.0.0",
    "source-map-support": "^0.4.8",
    "ts-node": "^5.0.0",
    "typescript": "^2.7.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
